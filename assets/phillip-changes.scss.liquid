.collapsible {
  background-color: white;
  color: #38373E;
  cursor: pointer;
  padding: 18px;
  width: 100%;
  border: none;
  text-align: left;
  outline: none;
  font-size: 15px;
  border-radius: 8px;
  display: table-cell;
}

.collapsible.active, .collapsible:hover {
  background-color: white;
  border: 0px;
}

.collapsible:after {
  content: '\002B';
  color: #38373E;
  font-weight: bold;
  float: right;
  margin-left: 5px;
  position: relative;
  top: -10px;
}

.collapsible.active:after {
  content: "\2212";
}

.collapsible:focus {
  background-color: white;
}

.collapsibleContent {
  padding: 0px 18px;
  max-height: 0;
  overflow: scroll;
  transition: max-height 0.2s ease-out;
  background-color: white;
  position: relative;
  top: -4px;
  border-radius: 0px 0px 6px 6px;
}

.collapsibleContent img{
  max-width: 50%;
  margin-bottom: 15px;
  display: block;
}

